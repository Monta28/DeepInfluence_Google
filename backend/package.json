{
  "name": "deep-influent-backend",
  "version": "1.0.0",
  "description": "Backend API pour la plateforme Deep Influent",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npx prisma generate && npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:normalize": "node scripts/backfill-normalized.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "api",
    "express",
    "prisma",
    "sqlite",
    "experts",
    "formations"
  ],
  "author": "Deep Influent Team",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.12.0",
    "@tanstack/react-query": "^5.85.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "react-hook-form": "^7.62.0",
    "socket.io": "^4.8.1",
    "stream-chat": "^8.16.0",
    "uuid": "^13.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "prisma": "^6.12.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
